{
    "name": "lunarphp/livewire-starter-kit",
    "type": "project",
    "description": "Livewire starter kit for Lunar e-commerce.",
    "keywords": [
        "lunar",
        "ecommerce",
        "e-commerce",
        "livewire",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "asantibanez/livewire-charts": "^4.1",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/cashier": "^15.7",
        "laravel/framework": "^10.43|^11.0",
        "laravel/sanctum": "^3.3|^4.0",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.0",
        "lunarphp/lunar": "^1.0",
        "lunarphp/stripe": "^1.0",
        "lunarphp/table-rate-shipping": "^1.0",
        "meilisearch/meilisearch-php": "^1.6",
        "predis/predis": "^2.2",
        "stripe/stripe-php": "^16.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0|^8.1",
        "phpunit/phpunit": "^10.1|^11.0.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
